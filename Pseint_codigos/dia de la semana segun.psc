// que dado numero entre 1 y 7 escriba su correspondiente dia de semana
Algoritmo sin_titulo
	Escribir 'digite un numero entre 1 y 7'
	Leer num
	Segun num  Hacer
		1:
			Escribir 'lunes'
		2:
			Escribir 'martes'
		3:
			Escribir 'miercoles'
		4:
			Escribir "jueves"
		5:
			Escribir "viernes"
		6:
			Escribir "sabado"
		7:
			Escribir "domingo"
		De Otro Modo:
			Escribir 'error interno'
	FinSegun
FinAlgoritmo

